{
    "name": "muhamed-didovic/shortener-frank",
    "type": "library",
    "description": "Shortener for Laravel with Vue.js SPA app",
    "keywords": [
        "muhamed-didovic",
        "shortener-frank"
    ],
    "homepage": "https://github.com/muhamed-didovic/shortener-frank",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhamed Didovic",
            "email": "muhamed.didovic@gmail.com",
            "homepage": "https://github.com/muhamed-didovic",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1",
        "illuminate/support": "~5|~6",
        "illuminate/validation": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MuhamedDidovic\\Shortener\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhamedDidovic\\Shortener\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "MuhamedDidovic\\Shortener\\ShortenerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
